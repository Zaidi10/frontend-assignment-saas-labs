    "@testing-library/user-event": "^14.5.2",
    "ajv": "^8.17.1"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "lines": 90
      }
    },
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/src/index.tsx",
      "/src/reportWebVitals.ts",
